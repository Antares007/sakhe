// @flow
import type {Stream} from '@most/types'
import type {PutRay as SuperPutRay} from './a'

export interface PutRay<A> extends SuperPutRay<Stream<A>> {}

export interface Pith<A> {(put: PutRay<A>): void}

export interface Bark<A, B> {(pith: Pith<A> | Stream<Pith<A>>): Stream<B>}

declare export default function tree<A, B>(
  deltac: (as: Stream<A>[]) => Stream<B>
): Bark<A, B>
