// @flow
import type {Stream} from '@most/types'

export type Ray<A> = (a: Stream<A>) => number

export type Pith<A> = (put: Ray<A>) => void

export type Bark<A, B> = (pith: Pith<A> | Stream<Pith<A>>) => Stream<B>

declare export default function tree<A, B>(
  deltac: (as: Stream<A>[]) => Stream<B>
): Bark<A, B>
